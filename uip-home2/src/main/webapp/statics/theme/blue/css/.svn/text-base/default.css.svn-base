/*aarhxz_theme:blue*/
@import url("../../common.css");
@charset "utf-8";
body,html{ height:100%; font-size:12px; color:#000;}
.nosrcoll{overflow: hidden;}

/*主框架header样式*/
.plat-header-wrap{padding:2px 0px 0px 0px; margin:0;height: 61px;line-height: 61px;background: #306fbc;clear: both;}
.plat-header{clear:both ;padding:0; margin:0;height: 61px;line-height: 61px;background: #ffffff;}
.plat-logo{float: left;width: 283px;height: 50px;background:url("../images/logo.png") no-repeat; margin-top: 5px}
.plat-menu-wrap{float: left;position: relative;}
/*主框架中间内容区域样式*/
.plat-content-wrap{padding: 1px 0px 0px 0px;margin: 0px;overflow:auto;position: relative;clear: both;}
.plat-content-bg{background: url("../images/bg/bg.jpg") no-repeat;}
.plat-content{padding: 0px;margin: 0px;}
.plat-center-panel{margin:0 auto;width:988px;position: relative;}
.plat-margin-top30{margin: 30px auto 30px auto}
.plat-margin-top50{margin: 50px auto 30px auto}
/*主框架footer样式*/
.plat-footer{ height: 53px;line-height: 53px; background: #e7e7e7;background: url("../images/bg-sub.png") repeat-x;}
.plat-footer-inner{text-align: center;color:#999999;}
.plat-footer-inner span.cc{font:normal 14px/22px 'Verdana'; }
.plat-bg-upload-bt{position: absolute;z-index:10;top: 15px;right: 15px;cursor: pointer;width: 31px;height: 28px;background:url("../images/bg-upload-bt.png") no-repeat; }


/*一级菜单控件的样式*/
ul.plat-menu{padding: 0px 5px 0px 0px;margin: 0px;list-style: none;}
ul.plat-menu li{
	float: left;
	margin: 0px 5px 0px 5px;
	width: 120px;
	text-align: center;
	cursor: pointer;
	white-space:nowrap;
 	text-overflow:ellipsis; 
 	-o-text-overflow:ellipsis; 
 	overflow: hidden;
}
ul.plat-menu li.split{height: 61px;width: 1px;background: #ffffff}
ul.plat-menu li a{font-size: 14px;color: #6f6d6e;text-decoration: none;font-weight: bold;}
ul.plat-menu li a:hover{text-decoration: none;color:#e73229}
ul.plat-menu li.selected{background:#306fbc;color:#fff }
ul.plat-menu li.selected a{color:#fff;/*border-bottom:1px dotted #fff;*/padding-bottom: 5px;}
div.menu-ext-bt {
	cursor: pointer;
	background: url("../images/menu-ext.png") no-repeat 0 22.5px;
	width: 80px;
	height: 61px;
	line-height: 61px;
	position:absolute;
	top: 0px;
	font-size:13px;
	font-weight:bold;
	padding-left: 20px;
	overflow:hidden;
	color: #306fbc;
}

div.plat-menu-ext-wrap {position: absolute;right: -60px;top:50px;z-index:2147483646;}
ul.plat-menu-ext-ul{padding: 0;margin: 0;list-style: none;}
ul.plat-menu-ext-ul li{
	border-color:#dcdce0;
	border-style:solid;
	border-width: 1px 1px 0px 1px;
	background: #ffffff;
	width: 160px;
	height: 30px;
	line-height: 30px;
	cursor: pointer;
	padding: 5px;
	text-align: center;
	white-space:nowrap;
 	text-overflow:ellipsis; 
 	-o-text-overflow:ellipsis; 
 	overflow: hidden;
}
ul.plat-menu-ext-ul li.last{border-width: 1px 1px 1px 1px}
ul.plat-menu-ext-ul li a{font-size: 14px;color: #6e6c6d;text-decoration: none;font-weight: bold;}
ul.plat-menu-ext-ul li a:hover{text-decoration: none;color:#306fbc;}
ul.plat-menu-ext-ul li.selected{background:#306fbc;}
ul.plat-menu-ext-ul li.selected a{color:#fff;}
/*纵向菜单样式*/
ul.plat-vertical-menu{padding:5px 0px 0px 0px;margin:0px;list-style: none;}
ul.plat-vertical-menu li{cursor:pointer;list-style: none;background: #ffffff;border-bottom: 1px solid #ebebeb;position: relative;}
ul.plat-vertical-menu li.noBorder{border-bottom-color: #fff}

ul.plat-vertical-menu div.selected{background:#85a7d1;color:#fff }
ul.plat-vertical-menu div.selected div.plat-vertical-menu-1-item-txt{color:#fff }

div.plat-vertical-menu-1-item{padding: 7px 0px 7px 0px}
div.plat-vertical-menu-1-item-icon{margin-left:10px;}
div.plat-vertical-menu-1-item-txt{
	font-size: 14px;
	font-weight: bold;
	color: #777472;
	padding-left: 10px;
	overflow:hidden;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
    white-space: nowrap;
    height: 18px;
    line-height: 18px;
}
div.plat-vertical-menu-1-expand{position: absolute;right: 5px;top: 15px;width: 20px;height: 20px;background: url("../images/treemenu/tree-ex-cp.png") -20px 0px no-repeat;cursor: pointer;}
div.plat-vertical-menu-1-cp{position: absolute;right: 5px;top: 15px;width: 20px;height: 20px;background: url("../images/treemenu/tree-ex-cp.png") 0px 0px no-repeat;cursor: pointer;}

ul.plat-vertical-2-menu{padding:0px 0px 0px 0px;margin:0px;list-style: none;border-top: 1px solid #ebebeb}
ul.plat-vertical-2-menu li{cursor:pointer;list-style: none;background: #ffffff;border-bottom: 1px solid #ebebeb;position: relative;}
ul.plat-vertical-2-menu div.selected{background:#85a7d1;color:#fff }
ul.plat-vertical-2-menu div.selected div.plat-vertical-menu-2-item-txt{color:#fff }
div.plat-vertical-menu-2-item{padding: 10px 0px 10px 0px}
div.plat-vertical-menu-2-item-icon{margin-left:0px;}
div.plat-vertical-menu-2-item-txt{
	font-size: 12px;
	line-height:14px;
	height:14px;
	color: #777472;
	padding-left: 10px;
	overflow:hidden;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
    white-space: nowrap;
}
div.plat-vertical-menu-2-expand{position: absolute;right: 5px;top: 10px;width: 20px;height: 20px;background: url("../images/treemenu/tree-ex-cp.png") -20px 0px no-repeat;cursor: pointer;}
div.plat-vertical-menu-2-cp{position: absolute;right: 5px;top: 10px;width: 20px;height: 20px;background: url("../images/treemenu/tree-ex-cp.png") 0px 0px no-repeat;cursor: pointer;}



/*主框架右上角工具栏样式*/
.plat-tools-wrap{float: right;margin: 18px 0px 0px 0px}
ul.plat-tools{padding: 0px 5px 0px 0px;margin: 0px;list-style: none;background: #dfe1de}
ul.plat-tools li{height:28px;line-height:28px;float: left;padding: 0px 10px 0px 10px;cursor: pointer;background: #dfe1de}
ul.plat-tools li a{font-size: 12px;color: #000;text-decoration: none;padding-left: 16px;}
ul.plat-tools li:hover a{
	color: #306fbc;
}
ul.plat-tools li.selected{background: #306fbc}
ul.plat-tools li.selected a{color: #fff}
/*工具栏的图标样式集合*/
div.plat-tools-bt-user{background:url(../images/tools-icon/user.png) no-repeat center center;width: 16px;display:inline-block; *zoom:1;*display:inline;}
div.plat-tools-bt-help{background:url(../images/tools-icon/help.png) no-repeat center center;width: 16px;display:inline-block; *zoom:1;*display:inline;}
div.plat-tools-bt-logout{background:url(../images/tools-icon/logout.png) no-repeat center center;width: 16px;display:inline-block; *zoom:1;*display:inline;}
div.plat-tools-bt-setting{background:url(../images/tools-icon/setting.png) no-repeat center center;width: 16px;display:inline-block; *zoom:1;*display:inline;}
div.plat-tools-bt-theme{background:url(../images/tools-icon/theme.png) no-repeat center center;width: 16px;display:inline-block; *zoom:1;*display:inline;}
div.plat-tools-bt-icodown{background:url(../images/tools-icon/ico_down.png) no-repeat center center;width: 6px;display:inline-block; *zoom:1;*display:inline;}
ul.plat-tools li.ThemeTools{position: relative;}
ul.plat-tools li.ThemeTools ul.plat-theme-list{left:18px; line-height: 27px; list-style-type: none; text-align: left; z-index:2147483646;  position: absolute;border-color: #CDCFCE;border-width: 1px;border-style:solid;display: none;}
ul.plat-tools li.ThemeTools ul.plat-theme-list li{float: none;cursor: pointer;background-color: white;}
ul.plat-tools li.ThemeTools ul.plat-theme-list.theme1 li{width: 54px;}
ul.plat-tools li.ThemeTools ul.plat-theme-list.setting1 li{width: 90px;}
ul.plat-tools li.ThemeTools ul.plat-theme-list li.nolast{border-bottom-width: 1px;border-bottom-style: dashed;border-bottom-color:#CDCFCE; }
ul.plat-tools li.ThemeTools ul.plat-theme-list li.sub{ }
ul.plat-tools li.ThemeTools ul.plat-theme-list li.hover{background-color:#85a7d1;}
ul.plat-tools li.ThemeTools ul.plat-theme-list li.selected{background-color: #959794;}
ul.plat-tools li.ThemeTools ul.plat-theme-list li div.themeico{width: 11px;display:inline-block; *zoom:1;*display:inline;}
ul.plat-tools li.ThemeTools ul.plat-theme-list li div.themeico.red{background:url(../images/tools-icon/theme_red.png) no-repeat center center;}
ul.plat-tools li.ThemeTools ul.plat-theme-list li div.themeico.blue{background:url(../images/tools-icon/theme_blue.png) no-repeat center center;}
ul.plat-tools li.ThemeTools ul.plat-theme-list li div.themeico.green{background:url(../images/tools-icon/theme_green.png) no-repeat center center;}
/*瀑布流扩展样式*/
ul.plat-waterfall-layout{
  list-style-type: none;
  position: relative; /** Needed to ensure items are laid out relative to this container **/
  margin: 0;
  padding: 0;
}
ul.plat-waterfall-layout li {
  background-color: #ffffff;
  border: 1px solid #dedede;
          border-radius: 2px;
     -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  display: none; /** Hide items initially to avoid a flicker effect **/
  cursor: pointer;
  padding: 4px;
}
.plat-home-waterfall-item{width: 310px;}
.plat-model-waterfall-item{width: 220px}
ul.plat-waterfall-layout li.selected{background: #85a7d1}
ul.plat-waterfall-layout li.selected div.plat-model-txt{color: #fff}
ul.plat-waterfall-layout li.hover{background: #85a7d1}
ul.plat-waterfall-layout li.hover div.plat-model-txt{color: #fff}
.wookmark-placeholder {
	border-radius: 2px;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	background-color: #eee;
	border: 1px solid #dedede;
	z-index: -1;
}
div.plat-waterfall-empty-container{
	width: 550px;
	height: 140px;
	line-height:150px;
	background-color: white;
	margin: 0 auto;
	margin-top: 80px;
}
