/*传统tab方式的内容*/
div.plat-tab-container{margin: 0px;padding: 0px;}






/**桌面样式**/
div.plat-desktop-panel{padding: 0px;margin: 0px;}
ul.plat-desktop-wrap{padding: 0px;margin: 0px;list-style: none;float: left;}
ul.plat-desktop-wrap li{z-index: 28;position:relative;margin: 5px;padding:1px;background: #fff;width: 100px;height:90px;cursor: pointer;}
ul.plat-desktop-wrap li.desktop-item-li{float: left;}
ul.plat-desktop-wrap li.desktop-add-bt{filter:alpha(opacity=70);opacity:0.7;}
ul.plat-desktop-wrap li:hover{background:#85a7d1;}
ul.plat-desktop-wrap li:hover div.desktop-item-txt{color:#fff;}
ul.plat-desktop-wrap li div.itemClose{z-index: 29; background:url("../images/tab-close.png") -17px 0px no-repeat ;display:block;width:17px;height:16px; position:absolute; right:2px; top:2px;cursor:pointer}

div.desktop-item{padding: 0;margin: 15px 5px 10px 5px;}
div.desktop-item-icon{width: 44px;height: 40px;}
div.desktop-item-txt{
	overflow:hidden;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
    white-space: nowrap;
	font-size: 12px;color: #827c7e;margin-top: 8px;
	line-height: 16px;
}
div.desktop-add-bt-icon{background:url(../images/menu-icon/blank.png) no-repeat center center;width: 44px;height: 40px; }
li.plat-desktop-placeholder{filter:alpha(opacity=30);opacity:0.3;float: left; margin: 5px;background: #fff;width: 100px;height:90px;}
li.plat-desktop-placeholder2{filter:alpha(opacity=30);opacity:0.3;margin: 5px;background: #fff;width: 100px;height:90px;}

/*自定义jquery.layout样式,add shendongsheng*/
.ui-layout-resizer-sliding {
	opacity: 0.1;
	filter: alpha(opacity = 10);
	background-color: #999;
}
.ui-layout-resizer-sliding:hover {
	opacity: 1;
	filter: alpha(opacity = 100);
}
.ui-layout-resizer-west-open{
	background-color: #DDD;
}
.ui-layout-resizer-west-dragging, .ui-layout-resizer-west-open:hover,.ui-layout-resizer-west-closed:hover {
	background: url(../images/jlayout/resizable-w.gif) repeat-y center;
}

.ui-layout-toggler-west-open {
	background: url(../images/jlayout/toggle-lt.gif) no-repeat center
		right;
}
.ui-layout-resizer-west-closed{
	background-color: #DDD;
}
.ui-layout-toggler-west-closed
{
	background: url(../images/jlayout/toggle-gt.gif) no-repeat center
		right;
}
.mytoggler-west{
	width: 15px;
	height: 84px;
	position: absolute;
	z-index: 9999;
}
.mytoggler-west.open{
	right:0px;
}
.mytoggler-west.closed{
	left:0px;
}
.mytoggler-west.open .mytoggler-west-inner{
	background-position:0px 0px;
}
.mytoggler-west.closed .mytoggler-west-inner{
	background-position:-30px 0px;
}
.mytoggler-west.open .mytoggler-west-inner.hover{
	background-position:-15px 0px;
	cursor:pointer;
}
.mytoggler-west.closed .mytoggler-west-inner.hover{
	background-position:-45px 0px;
	cursor:pointer;
}

 .mytoggler-west-inner{
	background-image: url("../images/jlayout/toggler-west.png");
	background-repeat:no-repeat;
	width: 15px;
	height: 84px;
}

